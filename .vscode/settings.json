{
  "[sql]": {
    "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "eslint.execArgv": ["--experimental-strip-types"],
  "eslint.options": {
    "flags": ["unstable_native_nodejs_ts_config"]
  },
  "eslint.runtime": "node",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "prisma.pinToPrisma6": true,
  "SQL-Formatter-VSCode.dialect": "sqlite"
}
