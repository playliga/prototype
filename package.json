{
  "name": "liga",
  "productName": "LIGA Esports Manager",
  "version": "3.0.0-beta.45",
  "description": "An immersive CS:GO Esports Simulator",
  "main": ".webpack/main",
  "author": "Steve Rivera",
  "license": "BUSL-1.1",
  "homepage": "https://playliga.gg",
  "repository": {
    "type": "git",
    "url": "https://github.com/playliga/prototype.git"
  },
  "bugs": {
    "url": "https://github.com/playliga/prototype/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "esports",
    "simulator",
    "csgo"
  ],
  "scripts": {
    "cli": "node --env-file .env -r ts-node/register -r tsconfig-paths/register cli",
    "cli:init": "openapi-typescript https://api.pandascore.co/openapi.json --output cli/generated/pandascore.ts",
    "docs": "npx typedoc --cleanOutputDir --entryPointStrategy expand src",
    "db:clean": "rm -f src/backend/prisma/saves/save_0.db",
    "db:generate": "prisma generate --schema src/backend/prisma/schema.prisma",
    "db:init": "prisma migrate dev --schema src/backend/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/backend/prisma/schema.prisma",
    "db:reset": "npm run db:clean && npm run db:init",
    "db:studio": "prisma studio --schema src/backend/prisma/schema.prisma",
    "gen:icon": "magick src/frontend/assets/icon.png -background none -resize 256x256 src/frontend/assets/icon.ico",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "postinstall": "npm run db:migrate && npm run cli:init",
    "publish": "electron-forge publish",
    "start": "electron-forge start"
  },
  "prettier": {
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register src/backend/prisma/seeds"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.8.0",
    "@electron-forge/maker-dmg": "7.8.0",
    "@electron-forge/maker-squirrel": "7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "7.8.0",
    "@electron-forge/plugin-fuses": "7.8.0",
    "@electron-forge/plugin-webpack": "7.8.0",
    "@electron-forge/publisher-github": "7.8.0",
    "@electron/fuses": "1.8.0",
    "@eslint/js": "9.26.0",
    "@tailwindcss/typography": "0.5.10",
    "@types/lodash": "4.17.16",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "daisyui": "4.6.2",
    "dotenv": "16.5.0",
    "electron": "36.1.0",
    "eslint": "9.26.0",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-react": "7.37.5",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "node-loader": "2.0.0",
    "openapi-typescript": "7.7.1",
    "postcss": "8.4.35",
    "postcss-loader": "8.1.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prisma": "5.1.0",
    "style-loader": "3.3.1",
    "tailwindcss": "3.4.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.0"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "@g-loot/react-tournament-brackets": "1.0.31-rc",
    "@logdna/tail-file": "3.0.1",
    "@node-steam/vdf": "2.2.0",
    "@prisma/client": "5.1.0",
    "chart.js": "4.4.6",
    "classnames": "2.5.1",
    "commander": "9.4.0",
    "compressing": "1.10.0",
    "countries-list": "2.6.1",
    "date-fns": "2.30.0",
    "dedent": "1.5.1",
    "duel": "3.2.0",
    "electron-is": "3.0.0",
    "electron-log": "5.4.0",
    "electron-squirrel-startup": "1.0.1",
    "flagpack": "1.0.5",
    "glob": "10.3.10",
    "groupstage": "3.1.1",
    "lodash": "4.17.21",
    "probability-pick": "1.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.1",
    "react-hot-toast": "2.4.1",
    "react-icons": "4.4.0",
    "react-markdown": "8.0.7",
    "react-router-dom": "6.22.3",
    "react-select": "5.7.0",
    "rehype-raw": "7.0.0",
    "sqlite3": "5.1.7",
    "squirrelly": "9.1.0"
  }
}
